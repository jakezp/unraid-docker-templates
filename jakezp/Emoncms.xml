<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Emoncms</Name>
  <Beta>False</Beta>
  <Category>Productivity:</Category>
  <Date>2016-05-27</Date>
  <Support>http://www.google.com</Support>
  <Overview>Energy monitor</Overview>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/r/jakezp/emoncms</Registry>
  <Repository>jakezp/emoncms</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>MYSQL_PASSWORD</Name>
      <Value>emoncms</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
	<Publish>
	  <Port>
        <HostPort>8998</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>3307</HostPort>
        <ContainerPort>3306</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
	<Volume>
      <HostDir></HostDir>
      <ContainerDir>/etc/mysql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/lib/mysql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/lib/phpfiwa</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/lib/phpfina</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/lib/phptimeseries</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/www/html</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8998]/</WebUI>
  <Banner>http://emoncms.org/Modules/site/emoncms_front.png</Banner>
  <Icon>http://emoncms.org/Modules/site/emoncms_front.png</Icon>
</Container>